# RPBox 环境配置示例
# 复制此文件为 .env 并填入实际值

# ========== 服务端配置 ==========
SERVER_PORT=8080
SERVER_MODE=debug

# ========== 数据库配置 ==========
DB_HOST=localhost
DB_PORT=5432
DB_USER=rpbox
DB_PASSWORD=your_password_here
DB_NAME=rpbox

# ========== JWT配置 ==========
JWT_SECRET=your_jwt_secret_change_in_production
JWT_EXPIRE_HOURS=72

# ========== Redis配置 ==========
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# ========== AI服务配置 ==========
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com/v1

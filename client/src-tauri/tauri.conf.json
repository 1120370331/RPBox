{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RPBox",
  "version": "0.1.0",
  "identifier": "com.rpbox.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3101"
  },
  "app": {
    "windows": [
      {
        "title": "RPBox",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ]
  },
  "plugins": {
    "dialog": null
  }
}
